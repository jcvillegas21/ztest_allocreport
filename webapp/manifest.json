        }
      },
      "ZV_API_ORDERBSD_02": {
        "uri": "/sap/opu/odata/sap/ZV_API_ORDERBSD_02/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/order/metadata.xml",
          "odataVersion": "2.0"
        }
      },
      "ZV_SRVACCESSUI": {
        "uri": "/sap/opu/odata4/sap/zv_ui_prodsubstaccessaatp_04/srvd/sap/zv_srvaccessui/0001/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/ZV_SRVACCESSUI/metadata.xml",
