    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "com.pg.s4.zaatpallocrpt.fiorizaatpallocrpt.view",
        "async": true
      },
      "routes": [
        {
          "name": "RouteAllocReportingMain",
          "pattern": ":?query:",
          "target": [
            "TargetAllocReportingMain"
          ]
        }
      ],
      "targets": {
        "TargetAllocReportingMain": {
          "id": "AllocReportingMain",
          "name": "AllocReportingMain"
        }
      }
    },
    "rootView": {
      "viewName": "com.pg.s4.zaatpallocrpt.fiorizaatpallocrpt.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
