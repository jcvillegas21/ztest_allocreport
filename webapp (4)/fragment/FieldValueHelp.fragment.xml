<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog id="_IDGenSelectDialog1"
		title="{dialogModel>/title}"
		class="sapUiPopupWithPadding"
		items="{
			path: 'dialogModel>/collectionPath'
			
		}"
		search="onValueHelpSearch"
		confirm="onValueHelpOkPress"
		cancel="onValueHelpCancelPress"
        multiSelect="true">

		<StandardListItem id="_IDGenStandardListItem"
			title="{path: 'dialogModel>/key'}"
			description="{path: 'dialogModel>/description'}"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>
